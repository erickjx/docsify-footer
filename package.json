{
  "name": "docsify-footer",
  "version": "0.0.1",
  "description": "A simple Footer plugin for Docsify",
  "main": "src",
  "scripts": {
    "build": "npm run build:dev && npm run build:prod",
    "build:dev": "webpack src/index.js -o dist/docsify-footer.js --mode=development && webpack src/index.js -o example/docsify-footer.js --mode=development",
    "build:prod": "webpack src/index.js -o dist/docsify-footer.min.js --mode=production",
    "example": "npx docsify serve example"
  },
  "files": [
    "src",
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erickjx/docsify-footer.git"
  },
  "author": "Erick JA",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/erickjx/docsify-footer/issues"
  },
  "homepage": "https://github.com/erickjx/docsify-footer#readme",
  "devDependencies": {
    "docsify-cli": "^4.4.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  },
  "directories": {
    "example": "example"
  },
  "dependencies": {},
  "keywords": [
    "Docsify",
    "Footer"
  ]
}
